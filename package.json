{
	"name": "google-that-for-me",
	"displayName": "Google That For Me",
  "description": "A VSCode extension that searches a text selection on Google for you",
  "version": "0.0.1",
  "publisher": "",
  "author": {
    "name": "Matheus Calegaro",
    "email": "hello@matheus.me",
    "url": "https://matheus.me"
  },
	"engines": {
		"vscode": "^1.38.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:extension.googleThatForMe"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [{
			"command": "extension.googleThatForMe",
			"title": "Google That For Me"
		}],
		"menus": {
			"editor/context": [{
				"command": "extension.googleThatForMe",
				"group": "YourGroup@1"
			}]
		}
	},
	"scripts": {
		"vscode:prepublish": "yarn run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "yarn run compile",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^5.2.6",
		"@types/node": "^10.12.21",
		"@types/vscode": "^1.38.0",
		"glob": "^7.1.4",
		"mocha": "^6.1.4",
		"typescript": "^3.3.1",
		"tslint": "^5.12.1",
		"vscode-test": "^1.0.2"
	}
}
